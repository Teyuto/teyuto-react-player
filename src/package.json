{
    "name": "@teyuto/react-native-player",
    "author": "teyuto",
    "version": "0.0.7-dev.0",
    "description": "React Native teyuto player",
    "repository": {
      "type": "git",
      "url": "https://github.com/teyuto/teyuto-react-player/"
    },
    "license": "MIT",
    "keywords": [
      "video",
      "player",
      "teyuto"
    ],
    "private": false,
    "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org/"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "tslint": "tslint --project .",
        "test": "jest",
        "build": "rollup -c",
